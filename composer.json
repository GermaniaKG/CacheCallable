{
	"name": "germania-kg/cachecallable",

	"description": "Callable convenience wrapper around PSR-6 Cache Item Pools: Seamlessly create, return, and store your data.",

	"type":        "library",
	"license":     "MIT",

	"keywords": [
		"PSR-6",
   	"Cache",
		"Callable",
		"PHP",
		"Caching",
		"Middleware"
	],

	"authors": [
		{
			"name":  "Carsten Witt",
			"email": "carstenwitt@germania-kg.de",
			"role":  "Developer"
		}
	],

	"autoload": {
		"psr-4": {
			"Germania\\Cache\\": "src"
		}
	},

  "autoload-dev": {
      "psr-4": {
      	"mocks\\": "tests/mocks/",
      	"tests\\": "tests/src/"
      }
  },
	"require": {
		"php": "^5.6|^7.0",
		"psr/cache": "^1.0",
		"psr/log": "^1.0",
		"tedivm/stash": "^0.15"
	},

    "require-dev": {
        "php": "^7.3",
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "php-coveralls/php-coveralls": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "spatie/phpunit-watcher": "^1.0"
    },

    "scripts": {
          "phpunit": "phpunit",

          "test": [
              "@phpunit"
          ]
    }
}
