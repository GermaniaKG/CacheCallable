{
	"name": "germania-kg/cachecallable",

	"description": "Callable convenience wrapper around PSR-6 Cache Item Pools: Seamlessly create, return, and store your data.",

	"type":        "library",
	"license":     "MIT",

	"keywords": [
		"PSR-6",
   	"Cache",
		"Callable",
		"PHP",
		"Caching",
		"Middleware"
	],

	"authors": [
		{
			"name":  "Carsten Witt",
			"email": "carstenwitt@germania-kg.de",
			"role":  "Developer"
		}
	],

	"autoload": {
		"psr-4": {
			"Germania\\Cache\\": "src"
		}
	},

  "autoload-dev": {
      "psr-4": {
      	"mocks\\": "tests/mocks/",
      	"tests\\": "tests/src/"
      }
  },

	"require-dev": {
			"satooshi/php-coveralls": "^1.0",
			"phpunit/phpunit": "^5.2",
		  "monolog/monolog": "^1.21"
	},
	"require": {
		"psr/cache": "^1.0",
		"psr/log": "^1.0"
	}
}
